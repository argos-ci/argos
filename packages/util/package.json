{
  "name": "@argos/util",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && swc src -d dist --strip-leading-paths",
    "watch-build": "npm run build -- --watch --quiet",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check --ignore-path .gitignore .",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "sideEffects": false,
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  }
}

{
  "root": true,
  "extends": ["smooth", "smooth/react"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/forbid-prop-types": "off",
    "react/static-property-placement": "off",
    "import/no-cycle": "off"
  },
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": []
      }
    }
  }
}

{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "console": "babel-node src/scripts/repl.js",
    "test:unit": "jest --runInBand",
    "test:watch": "yarn test:unit --watch",
    "web:dev": "pm2-docker start pm2.config.js",
    "web:build": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src",
    "review:dev": "babel-node ./node_modules/.bin/webpack-dev-server --config=src/review/webpack/browserDevelopmentConfig.js --progress",
    "review:build": "rm -rf server/static/review && babel-node ./node_modules/.bin/webpack --config=src/review/webpack/browserProductionConfig.js --progress",
    "review:icons": "babel-node src/scripts/generateIcons.js",
    "review:compress": "open -a ImageOptim src server/public",
    "argos": "argos upload screenshots/samples --token $ARGOS_TOKEN || true",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "api:db:console": "docker-compose run postgres psql -h postgres -U argos",
    "api:db:create": "babel-node src/scripts/db/create.js",
    "api:db:drop": "babel-node src/scripts/db/drop.js",
    "api:db:dump": "babel-node src/scripts/db/dump.js",
    "api:db:load": "babel-node src/scripts/db/load.js",
    "api:db:reset": "yarn api:db:drop && yarn api:db:create && yarn api:db:load",
    "api:db:truncate": "babel-node src/scripts/db/truncate.js",
    "heroku-postbuild": "yarn web:build && yarn review:build && yarn knex migrate:latest",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
    "amqplib": "^0.5.1",
    "assets-webpack-plugin": "^3.5.1",
    "aws-sdk": "^2.94.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-remove-properties": "^0.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-metrics": "^0.1.4",
    "chai": "^4.1.1",
    "classnames": "^2.2.5",
    "clear-require": "^2.0.0",
    "compression": "^1.7.0",
    "connect-redis": "^3.3.0",

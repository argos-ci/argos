{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "console": "babel-node scripts/repl.js",
    "dev": "yarn dev:foreman & yarn dev:review",
    "dev:foreman": "nf --procfile Procfile.dev --port 4001 start",
    "dev:review": "babel-node ./node_modules/.bin/webpack-dev-server --config=src/review/webpack/browserDevelopmentConfig.js --progress",
    "test": "./ci/test.sh",
    "test:unit": "jest --runInBand",
    "test:watch": "yarn test:unit -- --watch",
    "argos": "ARGOS_COMMIT=$TRAVIS_COMMIT ARGOS_BRANCH=$TRAVIS_BRANCH argos upload sandbox/argos/screenshots --token $ARGOS_TOKEN || true",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "db:console": "docker-compose run postgres psql -h postgres -U argos",
    "db:create": "babel-node scripts/db/create.js",
    "db:drop": "babel-node scripts/db/drop.js",
    "db:dump": "babel-node scripts/db/dump.js",
    "db:load": "babel-node scripts/db/load.js",
    "db:migrate:latest": "babel-node node_modules/.bin/knex migrate:latest",
    "db:migrate:rollback": "babel-node node_modules/.bin/knex migrate:rollback",
    "db:truncate": "babel-node scripts/db/truncate.js",
    "db:seed": "knex seed:run",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:load",
    "build": "yarn build:server && yarn build:review",
    "build:server": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src",
    "build:review": "rm -rf server/static/review && babel-node ./node_modules/.bin/webpack --config=src/review/webpack/browserProductionConfig.js --progress",
    "heroku-postbuild": "yarn build && yarn db:migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": "7.2.0"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
    "amqplib": "^0.5.1",
    "assets-webpack-plugin": "^3.5.1",
    "aws-sdk": "^2.27.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-react-remove-properties": "^0.2.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "browser-metrics": "^0.1.2",
    "chai": "^3.5.0",
    "clear-require": "^2.0.0",
    "compression": "^1.6.2",
    "connect-redis": "^3.2.0",
    "convict": "^2.0.0",
    "ejs": "^2.5.6",
    "enzyme": "^2.7.1",
    "express": "^4.15.2",
    "express-err": "^1.0.1",
    "express-graphql": "^0.6.3",
    "express-session": "^1.15.1",
    "express-subdomain": "^1.0.5",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "github": "^9.2.0",
    "graphql": "^0.9.1",
    "heroku-node-settings": "^1.0.2",
    "html-minifier": "^3.4.0",
    "image-diff": "^1.6.3",
    "jss": "^6.4.0",
    "knex": "^0.12.7",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-alpha.7",
    "mime": "^1.3.4",
    "minimist": "^1.2.0",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "multer-s3": "^2.5.0",
    "mz": "^2.6.0",
    "objection": "^0.7.5",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "pg": "^6.1.4",
    "pretty-error": "^2.0.3",
    "promirepl": "^1.0.1",
    "raven": "^1.1.4",
    "raven-js": "^3.12.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "recompact": "^2.2.2",
    "redis": "^2.7.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.2.0",
    "sinon": "^1.17.7",
    "tmp": "^0.0.31",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "argos-cli": "^0.0.3",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.10.0",
    "factory-girl": "^4.2.1",
    "foreman": "^2.0.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "selenium-webdriver": "^3.3.0",
    "supertest": "^3.0.0",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "notify": false,
    "roots": [
      "<rootDir>/src",
      "<rootDir>/packages"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/src/server/bootstrap/setupTest.js"
    ]
  }
}

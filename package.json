{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "console": "babel-node scripts/repl.js",
    "dev": "yarn dev:foreman & yarn dev:review",
    "dev:foreman": "nf --procfile Procfile.dev --port 4001 start",
    "dev:review": "babel-node ./node_modules/.bin/webpack-dev-server --config=src/review/webpack/browserDevelopmentConfig.js --progress",
    "test:unit": "jest --runInBand",
    "test:watch": "yarn test:unit -- --watch",
    "argos": "argos upload screenshots/sample --token $ARGOS_TOKEN || true",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "db:console": "docker-compose run postgres psql -h postgres -U argos",
    "db:create": "babel-node scripts/db/create.js",
    "db:drop": "babel-node scripts/db/drop.js",
    "db:dump": "babel-node scripts/db/dump.js",
    "db:load": "babel-node scripts/db/load.js",
    "db:migrate:latest": "babel-node node_modules/.bin/knex migrate:latest",
    "db:migrate:rollback": "babel-node node_modules/.bin/knex migrate:rollback",
    "db:truncate": "babel-node scripts/db/truncate.js",
    "db:seed": "knex seed:run",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:load",
    "build": "yarn build:server && yarn build:review",
    "build:server": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src",
    "build:review": "rm -rf server/static/review && babel-node ./node_modules/.bin/webpack --config=src/review/webpack/browserProductionConfig.js --progress",
    "build:icons": "babel-node scripts/generateIcons.js",
    "heroku-postbuild": "yarn build && yarn db:migrate:latest",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write --no-semi --single-quote --trailing-comma es5 --print-width 100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": "7.7.3"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
    "@risingstack/trace": "^3.12.2",

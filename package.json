{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "yarn dev:foreman & yarn dev:review",
    "dev:foreman": "nf --procfile Procfile.dev --port 4001 start",
    "dev:review": "babel-node ./node_modules/.bin/webpack-dev-server --config=src/review/webpack/browserDevelopmentConfig.js --progress",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "jest --runInBand",
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "db:migrate": "knex migrate:latest",
    "db:dump": "docker-compose exec postgres pg_dump -s -U development > db/structure.sql",
    "db:load": "docker exec -i `docker-compose ps -q postgres` psql -U development < db/structure.sql",
    "db:seed": "knex seed:run",
    "build": "yarn build:server && yarn build:review",
    "build:server": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src",
    "build:review": "rm -rf server/static/review && babel-node ./node_modules/.bin/webpack --config=src/review/webpack/browserProductionConfig.js --progress",
    "heroku-postbuild": "yarn build && yarn db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": "7.2.0"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
    "amqplib": "^0.5.1",
    "assets-webpack-plugin": "^3.5.1",
    "aws-sdk": "^2.7.27",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-react-remove-properties": "^0.2.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "browser-metrics": "^0.1.2",
    "compression": "^1.6.2",
    "connect-redis": "^3.2.0",
    "convict": "^2.0.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-err": "^1.0.1",
    "express-graphql": "^0.6.1",
    "express-session": "^1.14.2",
    "express-subdomain": "^1.0.5",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "graphql": "^0.8.2",
    "html-minifier": "^3.2.3",
    "image-diff": "^1.6.3",
    "jss": "^6.2.0",
    "knex": "^0.12.6",
    "material-ui-build-next": "git://github.com/oliviertassinari/material-ui.git#next-splitme",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "multer-s3": "^2.5.0",
    "mz": "^2.6.0",
    "objection": "^0.6.2",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "pg": "^6.1.2",
    "raven": "^1.1.1",
    "raven-js": "^3.9.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "recompact": "^1.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-observable": "^0.13.0",
    "rxjs": "^5.0.3",
    "webpack": "^2.2.0-rc.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.9.0",
    "foreman": "^2.0.0",
    "jest": "^18.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "selenium-webdriver": "^3.0.1",
    "supertest": "^2.0.1",
    "tmp": "^0.0.31",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "jest": {
    "notify": true,
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/src/server/bootstrap/setupTest.js"
    ]
  }
}

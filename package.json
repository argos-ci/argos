{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "server:development": "nodemon src/server/bootstrap/development",
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "jest",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "review:browser:development": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=src/review/webpack/browserDevelopmentConfig.js --progress",
    "heroku-postbuild": "yarn build:all && yarn db:migrate",
    "build:all": "yarn build:lib",
    "build:lib": "rm -rf lib/ && cp -R src/ lib/ && NODE_ENV=server babel -d lib src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": "7.2.0"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
    "amqplib": "^0.5.1",
    "app-module-path": "^2.1.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "compression": "^1.6.2",
    "convict": "^1.5.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-err": "^1.0.1",
    "express-graphql": "^0.6.1",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "graphql": "^0.8.2",
    "html-webpack-plugin": "^2.24.1",
    "image-diff": "^1.6.3",
    "knex": "^0.12.6",
    "material-ui-build-next": "git://github.com/oliviertassinari/material-ui.git#next-splitme",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "objection": "^0.6.2",
    "pg": "^6.1.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^3.0.0",
    "recompose": "^0.21.0",
    "webpack": "^2.2.0-rc.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^18.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "selenium-webdriver": "^3.0.1",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "jest": {
    "notify": true,
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}

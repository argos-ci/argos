{
  "name": "@argos/backend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && swc src -d dist --strip-leading-paths",
    "watch-build": "npm run build -- --watch --quiet",
    "setup": "npm run db:create && npm run db:load",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check --cache --ignore-path=../../.gitignore --ignore-path=../../.prettierignore .",
    "lint": "eslint --ignore-path=../../.gitignore .",
    "db:check-structure": "knex-scripts check-structure",
    "db:create": "knex-scripts create",
    "db:drop": "knex-scripts drop",
    "db:dump": "npm run db:migrate:latest && knex-scripts dump",
    "db:load": "knex-scripts load",
    "db:seed": "knex seed:run",
    "db:truncate": "knex-scripts truncate",
    "db:migrate:latest": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:migrate:make": "knex migrate:make",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:load",
    "watch-server-build-and-synchronize": "node --watch ./dist/processes/proc/build-and-synchronize.js",
    "watch-server-screenshot-diff": "node --watch ./dist/processes/proc/screenshot-diff.js",
    "watch-server-web": "node --watch ./dist/processes/proc/web.js",
    "watch-server": "sleep 2 && concurrently \"npm:watch-server-*\""
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@argos/tsconfig": "*",
    "@argos/util": "*",
    "@aws-sdk/client-s3": "^3.535.0",
    "@aws-sdk/s3-request-presigner": "^3.535.0",
    "@gitbeaker/rest": "^39.34.3",
    "@graphql-tools/schema": "^10.0.3",
    "@octokit/auth-app": "^6.0.4",
    "@octokit/core": "^6.0.1",
    "@octokit/openapi-types": "^20.0.0",
    "@octokit/plugin-retry": "^7.0.3",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^13.1.1",
    "@octokit/webhooks-types": "^7.4.0",
    "@react-email/components": "^0.0.15",
    "@react-email/render": "^0.0.12",
    "@sentry/node": "^7.107.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/ejs": "^3.1.5",
    "ajv": "^8.12.0",
    "amqplib": "^0.10.3",
    "auth-header": "^1.0.0",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-err": "^3.0.0",
    "express-rate-limit": "^7.2.0",
    "google-auth-library": "^9.7.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash-es": "^4.17.21",
    "mime": "^4.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "objection": "^3.1.4",
    "odiff-bin": "^2.6.1",
    "p-retry": "^6.2.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "redis": "^4.6.13",
    "resend": "^3.2.0",
    "sharp": "^0.33.2",
    "stripe": "^14.21.0",
    "tmp": "^0.2.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/auth-header": "^1.0.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/convict": "^6.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/mime": "^3.0.4",
    "@types/morgan": "^1.9.9",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/tmp": "^0.2.6",
    "concurrently": "^8.2.2",
    "factory-girl-ts": "^2.3.0",
    "knex-scripts": "^0.3.6",
    "moment": "^2.30.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4"
  }
}
